{
  "name": "Journalen Coon",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "The Coon takes over Journalen!",
  "homepage_url": "http://en.wikipedia.org/wiki/The_Coon",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/background/background.html" ,
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Journalen Coon!",
    "default_popup": "src/browser_action/index.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
  ],

  "web_accessible_resources": [
    "src/*"
  ],

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"

}
